      <xs:enumeration value="tavush"/>
      <xs:enumeration value="vayotsDzor"/>
      <xs:enumeration value="notUsed"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="ReceiverRegionControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="yerevan"/>
      <xs:enumeration value="aragatsotn"/>
      <xs:enumeration value="ararat"/>
