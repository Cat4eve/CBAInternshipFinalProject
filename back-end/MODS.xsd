<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="mods">
    <xs:complexType>
      <xs:sequence>

        <!-- 1. Փոխանցման կամ ստացման բնույթ -->
        <xs:element name="transferTypeInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="transferType" type="TransferTypeControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 2. Փոխանցողի կամ ստացողի իրավական կարգավիճակ -->
        <xs:element name="legalStatusInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="legalStatus" type="LegalStatusControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 3. Գործարքի տեսակ -->
        <xs:element name="transactionTypeInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="transactionType" type="TransactionTypeControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 4. Փոխանցման կամ ստացման եղանակ -->
        <xs:element name="methodOfTransferInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="methodOfTransfer" type="MethodOfTransferControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 5. Փոխանցման կամ ստացման միջոց կամ համակարգ -->
        <xs:element name="transferSystemInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="transferSystem" type="TransferSystemFlexible"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 6. Փոխանցման կամ ստացման կատարման համար կիրառվող կապի միջոց -->
        <xs:element name="communicationTypeInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="communicationType" type="CommunicationTypeControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 7. Փոխանցման մարզ  -->
        <xs:element name="senderRegionInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="senderRegion" type="SenderRegionControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 8. Ստացման մարզ   -->
        <xs:element name="receiverRegionInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="receiverRegion" type="ReceiverRegionControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>


        <!-- 9. Փոխանցման կամ ստացման երկրի կոդ -->
        <xs:element name="ISO3166_1Alpha3CountriesInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ISO3166_1Alpha3Countries" type="ISO3166_1Alpha3CountriesControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 10. Փոխանցման կամ ստացման գումար -->
        <xs:element name="moneyInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="money" type="xs:string"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>
        
        <!-- 11. Փոխանցման կամ ստացման արժույթի կոդ -->
        <xs:element name="ISO4217CurrenciesInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ISO4217Currencies" type="ISO4217CurrenciesControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 12. Փոխանցման կամ ստացման գումարն արտահայտված ԱՄՆ դոլարով (հազար դոլար) -->
        <xs:element name="moneyToUSDInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="moneyToUSD" type="xs:string"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 13. Փոխանցման կամ ստացման գումարի մեծության դասակարգիչ (ԱՄՆ դոլարին համարժեք) -->
        <xs:element name="sizeClassifierInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="sizeClassifier" type="SizeClassifierControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 14. Փոխանցող կամ ստացող իրավաբանական անձի անվանում -->
        <xs:element name="legalEntityNameInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="legalEntityName" type="LegalEntityNameFlexible"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 15. Փոխանցող կամ ստացող իրավաբանական անձի  ՀՎՀՀ -->
        <xs:element name="taxPayerRegistrationNumberInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="taxPayerRegistrationNumber" type="TaxPayerRegistrationNumberFlexible"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 16. Փոխանցողի կամ ստացողի հատվածային պատկանելիություն -->
        <xs:element name="segmentMembershipInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="segmentMembership" type="SegmentMembershipControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 17. Փոխանցողի կամ ստացողի ռեզիդենտության երկիր -->
        <xs:element name="residencyInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="residency" type="ResidencyFlexible"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 18. Փոխանցման կամ ստացման գործարքի կողմ հանդիսացող իրավաբանական անձի անվանում -->
        <xs:element name="sideLegalEntityNameInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="sideLegalEntityName" type="xs:string"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 19. Փոխանցման կամ ստացման գործարքի կողմ հանդիսացողի ռեզիդենտության երկիր -->
        <xs:element name="sideResidencyInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="sideResidency" type="ResidencyFlexible"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>
        
        <!-- 20. Փոխանցման կամ ստացման գործարքի կողմ հանդիսացողի իրավական կարգավիճակ -->
        <xs:element name="sideLegalStatusInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="sideLegalStatus" type="LegalStatusControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 21. Փոխանցման կամ ստացման նպատակի դասակարգում -->
        <xs:element name="intentClassifierInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="intentClassifier" type="IntentClassifierControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 22. Փոխանցման կամ ստացման նպատակի կոդ -->
        <xs:element name="intentCodeInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="intentCode" type="IntentCodeControlled"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 23. Փոխանցման կամ ստացման քանակ -->
        <xs:element name="transferAmountInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="transferAmount" type="xs:int"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>

        <!-- 24. Փոխանցման կամ ստացման դիմաց վճարված միջնորդավճար -->
        <xs:element name="sidePaymentInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="sidePayment" type="xs:int"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Controlled Types -->
  <xs:simpleType name="TransferTypeControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="international"/>
      <xs:enumeration value="intercountry"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="LegalStatusControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="legalEntity"/>
      <xs:enumeration value="naturalPerson"/>
      <xs:enumeration value="solePropriator"/>
      <xs:enumeration value="notUsed"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="TransactionTypeControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="sent"/>
      <xs:enumeration value="received"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="MethodOfTransferControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="bankAccount"/>
      <xs:enumeration value="recordAccount"/>
      <xs:enumeration value="cash"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="TransferSystemFlexible">
    <xs:union memberTypes="TransferSystemControlled xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="TransferSystemControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="correspondentAccounts"/>
      <xs:enumeration value="foreignBranches"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CommunicationTypeControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SWIFT"/>
      <xs:enumeration value="internet"/>
      <xs:enumeration value="specialLine"/>
      <xs:enumeration value="SPFS"/>
      <xs:enumeration value="fax"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SenderRegionControlled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="yerevan"/>
      <xs:enumeration value="aragatsotn"/>
      <xs:enumeration value="ararat"/>
